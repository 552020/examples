type SchnorrAlgorithm = 
 variant {
   bip340secp256k1;
   ed25519;
 };
service : {
  for_test_only_change_management_canister_id: (text) ->
   (variant {
      Err: text;
      Ok;
    });
  public_key: (SchnorrAlgorithm) ->
   (variant {
      Err: text;
      Ok: record {public_key_hex: text;};
    });
  sign: (text, SchnorrAlgorithm) ->
   (variant {
      Err: text;
      Ok: record {signature_hex: text;};
    });
}
